<?xml version="1.0" encoding="UTF-8"?>

<Shell
    x:Class="ListItMaui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:ListIt_Maui.Views"
    Shell.FlyoutBehavior="Disabled"
    Shell.NavBarIsVisible="False"
    Title="ListItMaui">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate views:ProductPage}"
        Route="ProductPageShell" />

    <ShellContent 
        Title="AddProdut" 
        ContentTemplate="{DataTemplate views:AddProductPage}" 
        Route="AddProductShell" />

</Shell>